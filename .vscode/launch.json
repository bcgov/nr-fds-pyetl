    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "spar-extract",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/data_prep/main_spar_extract.py",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "spar-ingest",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/data_prep/main_spar_ingest.py",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env"
        }

    ]
}